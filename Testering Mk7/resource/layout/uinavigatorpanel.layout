"resource/layout/uinavigatorpanel.layout" {

	styles {


			"Label" {
				bgcolor = "0 0 60 255"
			}

		// Navigation Panel ---------------------------------------------------------------------------------------------------------

			// Navigation Panel
			"CUINavigatorPanel" {
				bgcolor = "0 60 0 255"
			}

		// Nav Buttons ---------------------------------------------------------------------------------------------------------

			// Nav Button
			"CUINavButton" {
				textcolor = "Labelfocus"
				bgcolor = "none"
				font-family = "basefont"
				font-size = "24"
				font-weight = "400"
			}

		// Url ---------------------------------------------------------------------------------------------------------

			// Url - Ev Cert (Not OSX)
			"LabelEVCert" [!$OSX] {
				font-family = "basefont"
				font-size = "14"
				textcolor = "darkclientbg"
				font-weight = "400"
				bgcolor = "html.secureurl"
				inset-left = "4"
				inset-right = "4"
				inset-top = "3"
			}

			// Url - Ev Cert (OSX)
			"LabelEVCert" [$OSX] {
				font-family = "basefont"
				font-size = "14"
				textcolor = "darkclientbg"
				font-weight = "400"
				bgcolor = "html.secureurl"
				inset-left = "3"
				inset-right = "4"
				inset-top = "3"
			}

			// Url - Broken Secure
			"URLBarBrokenSecure" {
				textcolor = "html.insecureurl"
			}

			// Url - Secure
			"URLBarSecure" {
				textcolor = "html.secureurl"
			}

			// Url - Status Image
			"URLStatusImage" {
				bgcolor = "none"
				inset = "0 0 0 0"
				render_bg = {}
				image = "graphics/html_lock_disabled"
			}

		// Nav Label ---------------------------------------------------------------------------------------------------------

			// Nav Label
			"NavLabel" {
				font-family = "basefont"
				font-size = "14" [!$OSX]
				font-size = "15" [$OSX]
				font-weight = "400"
				textcolor = "Label"
				font-style = "uppercase"
			}

			// Nav Label 2 (Not OSX)
			"NavLabel2" [!$OSX] {
				font-family = "basefont"
				font-size = "14"
				font-weight = "400"
				textcolor = "labelfocus"
				font-style = "uppercase"
				}

			// Nav Label 2 (OSX)
			"NavLabel2" [$OSX] {
				font-family = "basefont"
				font-size = "13"
				font-weight = "400"
				textcolor = "labelfocus"
				font-style = "uppercase"
				}

			// Nav Label 2 - Hover
			"NavLabel2:hover" {
				textcolor = "texthover"
				font-style = "outerglow,uppercase"
				font-outerglow-color = "TextGlowHover"
				font-outerglow-offset = "1"
				font-outerglow-filtersize = "3"
			}

			// Nav Label 2 - Selected
			"NavLabel2:selected" {
				textcolor = "white"
				font-style = "outerglow,uppercase"
				font-outerglow-color = "TextGlowHover"
				font-outerglow-offset = "1"
				font-outerglow-filtersize = "3"
			}

			// Nav Label - View (Not OSX)
			"NavLabelView" [!$OSX] {
				font-family = "basefont"
				font-size = "14"
				font-weight = "400"
				textcolor = "LabelDisabled"
				font-style = "uppercase"
				padding-left = "0"
			}

			// Nav Label - View (OSX)
			"NavLabelView" [$OSX] {
				font-family = "basefont"
				font-size = "13"
				font-weight = "400"
				textcolor = "LabelDisabled"
				font-style = "uppercase"
				padding-left = "0"
			}

			// Nav Label - Zoom (Not OSX)
			"NavLabelZoom" [!$OSX] {
				font-family = "basefont"
				font-size = "14"
				font-weight = "400"
				textcolor = "LabelDisabled"
				font-style = "uppercase"
				padding-right = "5"
			}

			// Nav Label - Zoom (OSX)
			"NavLabelZoom" [$OSX] {
				font-family = "basefont"
				font-size = "13"
				font-weight = "400"
				textcolor = "LabelDisabled"
				font-style = "uppercase"
				padding-right = "5"
			}

		// Music ---------------------------------------------------------------------------------------------------------

			// Music - Add Button
			"MusicAddButton" {
				font-size = "14"
				font-size = "13" [$OSX]
				font-family = "basefont"
				textcolor = "labelfocus"
				font-style = "uppercase"
				render_bg {
					0 = "image( x0, y0 + 2, x1, y1, graphics/icon_collapse )"
				}
			}

			// Music - Add Button - Hover
			"MusicAddButton:hover" {
				textcolor = "white"
				render_bg {
					0 = "image( x0, y0 + 2, x1, y1, graphics/icon_collapse_over )"
				}
			}

			// Music - Player Icon
			"MusicPlayerIcon" {
				bgcolor = "none"
				inset = "-1 0 0 0"
				render_bg = {}
				image = "graphics/icon_music_player"
				padding-left = "0"
				padding-right = "-1"
				padding-top = "-1"
			}

			// Music - Player Icon - Hover
			"MusicPlayerIcon:hover" {
				bgcolor = "none"
				inset = "-1 0 0 0"
				render_bg = {}
				image = "graphics/icon_music_player_hover"
				padding-left = "0"
				padding-right = "-1"
				padding-top = "-1"
			}

			// Music - Player Icon - Selected
			"MusicPlayerIcon:selected" {
				bgcolor = "none"
				inset = "-1 0 0 0"
				render_bg = {}
				image = "graphics/icon_music_player_selected"
				padding-left = "0"
				padding-right = "-1"
				padding-top = "-1"
			}

		// Library ---------------------------------------------------------------------------------------------------------

			// Library - Search
			"LibrarySearch" {
				padding-left = "-4"
				font-family = "basefont"
				font-size = "14"
				font-weight = "400"
				textcolor = "text"
			}

			// Library - Search - Hover
			"LibrarySearch:hover" {
				textcolor = "texthover"
			}

			// Library - Search - Disabled
			"LibrarySearch:disabled" {
				font-style = "italic"
				textcolor = "None"
			}

			// Library - Search - Empty
			"LibrarySearch:empty" {
				font-style = "italic"
				textcolor = "LabelDisabled"
			}

			// Library - Search - Empty - Hover
			"LibrarySearch:empty:hover" {
				font-style = "italic"
				textcolor = "texthover"
			}

			// Library - Search - Icon
			"LibrarySearchIcon" {
				bgcolor = "none"
				inset = "-1 0 0 0"
				render_bg = {}
				image = "graphics/icon_button_search"
				padding-left = "0"
				padding-right = "-1"
				padding-top = "-1"
			}

			// Library - Search - Icon
			"LibrarySearchIcon:disabled" {
				bgcolor = "none"
				inset = "-2 0 0 0"
				render_bg = {}
				image = "none"
				padding-left = "0"
				padding-right = "-1"
			}

			// Library - Separator
			"LibrarySeparator" {
				padding-left = "0"
				font-family = "basefont"
				font-size = "16"
				font-weight = "400"
				textcolor = "LabelDisabled"
				inset = "0 -2 0 0"
			}

			// Library - Show (Not OSX)
			"LibraryShow" [!$OSX] {
				padding-left = "0"
				font-family = "basefont"
				font-size = "14"
				font-weight = "400"
				textcolor = "LabelDisabled"
				font-style = "uppercase"
			}

			// Library - Show (OSX)
			"LibraryShow" [$OSX] {
				padding-left = "0"
				font-family = "basefont"
				font-size = "13"
				font-weight = "400"
				textcolor = "LabelDisabled"
				font-style = "uppercase"
			}

		// Text Entry ---------------------------------------------------------------------------------------------------------

			// Text Entry
			"TextEntry" {
				font-family = "basefont"
				font-size = "14"
				textcolor = "Text"
				font-weight = "400"
				bgcolor = "none"
				selectedtextcolor = "white"
				selectedbgcolor = "TextSelectedBG"
				shadowtextcolor = "Text"
				inset-left = "4"
				inset-top = "0"
				render {}
			}

			// Text Entry - Hover
			"TextEntry:hover" {
				textcolor = "TextHover"
			}

			// Text Entry - Active
			"TextEntry:active" {
				textcolor = "TextHover"
			}

			// Text Entry - Selected
			"TextEntry:selected" {
				textcolor = "TextHover"
			}

			// Text Entry - Url (Not OSX)
			"TextEntryURL" [!$OSX] {
				font-family = "basefont"
				inset-top = "1"
				inset-left = "2"
				font-size = "14"
				textcolor = "label"
				font-weight = "400"
				bgcolor = "none"
				selectedtextcolor = "TextEntrySelected"
				selectedbgcolor = "TextSelectedBG"
				shadowtextcolor = "Text"
			}

			// Text Entry - Url (OSX)
			"TextEntryURL" [$OSX] {
				font-family = "basefont"
				inset-top = "1"
				inset-left = "2"
				font-size = "14"
				textcolor = "label"
				font-weight = "400"
				bgcolor = "none"
				selectedtextcolor = "TextEntrySelected"
				selectedbgcolor = "TextSelectedBG"
				shadowtextcolor = "Text"
			}

		// View Details ---------------------------------------------------------------------------------------------------------

			// View Details - Button
			"ViewDetailButton" {
				bgcolor = "none"
				inset = "1 0 0 0"
				render_bg = {}
				image = "graphics/icon_button_detail"
				padding-left = "0"
				padding-right = "0"
			}

			// View Details - Button - Hover
			"ViewDetailButton:hover" {
				bgcolor = "none"
				render_bg = {}
				image = "graphics/icon_button_detail_over"
			}

			// View Details - Button - Active
			"ViewDetailButton:active" {
				bgcolor = "none"
				render_bg = {}
				image = "graphics/icon_button_detail_down"
			}

			// View Details - Button - Selected
			"ViewDetailButton:selected" {
				bgcolor = "none"
				render_bg = {}
				image = "graphics/icon_button_detail_down"
			}

			// View Details - Button - Disabled
			"ViewDetailButton:disabled" {
				bgcolor = "ButtonFace"
				render_bg = {}
				image = "graphics/icon_button_detail_disabled"
			}

		// View Grid ---------------------------------------------------------------------------------------------------------


			// View Grid - Button
			"ViewGridButton" {
				bgcolor = "none"
				inset = "1 0 0 0"
				render_bg = {}
				image = "graphics/icon_button_grid"
				padding-left = "0"
				padding-right = "0"
			}

			// View Grid - Button - Hover
			"ViewGridButton:hover" {
				bgcolor = "none"
				render_bg = {}
				image = "graphics/icon_button_grid_over"
			}

			// View Grid - Button - Active
			"ViewGridButton:active" {
				bgcolor = "none"
				render_bg = {}
				image = "graphics/icon_button_grid_down"
			}

			// View Grid - Button - Selected
			"ViewGridButton:selected" {
				bgcolor = "none"
				render_bg = {}
				image = "graphics/icon_button_grid_down"
			}

			// View Grid - Button - Disabled
			"ViewGridButton:disabled" {
				bgcolor = "none"
				render_bg = {}
				image = "graphics/icon_button_grid_disabled"
			}

		// View List ---------------------------------------------------------------------------------------------------------

			// View List - Button
			"ViewListButton" {
				bgcolor = "none"
				inset = "1 0 0 0"
				render_bg = {}
				image = "graphics/icon_button_list"
				padding-left = "0"
				padding-right = "0"
			}

			// View List - Button - Hover
			"ViewListButton:hover" {
				bgcolor = "none"
				render_bg = {}
				image = "graphics/icon_button_list_over"
			}

			// View List - Button - Active
			"ViewListButton:active" {
				bgcolor = "none"
				render_bg = {}
				image = "graphics/icon_button_list_down"
			}

			// View List - Button - Selected
			"ViewListButton:selected" {
				bgcolor = "none"
				render_bg = {}
				image = "graphics/icon_button_list_down"
			}

			// View List - Button - Disabled
			"ViewListButton:disabled" {
				bgcolor = "none"
				render_bg = {}
				image = "graphics/icon_button_list_disabled"
			}

		// Other ---------------------------------------------------------------------------------------------------------


			// Grouper
			"grouper" {
				bgcolor = "none"
				render_bg {
					0 = "fill( x0 + 2, y0 + 1, x0 + 273, y1 - 1, clientgrouper )"
					1 = "fill( x0 + 279, y0 + 1, x1 - 10, y1 - 1, clientgrouper )"
				}
			}

			// Header Text Large
			"headertextlarge" {
				padding-left = "10"
				padding-top = "5"
				padding-right = "10"
				padding-bottom = "5"
				font-size = "30"
				textcolor = "Text"
				font-weight = "400"
				bgcolor = "DarkDialogBGTransparent"
				minimum-width = "200"
			}

			// Panel Transparent
			"Panel_transparent" {
				bgcolor = "none"
			}

			// Zoom Slider
			"zoomslider" {
				inset = "0 -1 0 0"
				textcolor = "navlabel"
			}

			// Zoom Slider - Hover
			"zoomslider:hover" {
				inset = "0 -1 0 0"
				textcolor = "navlabel"
			}


	}

	layout {

		// Pages ---------------------------------------------------------------------------------------------------------

			// Web Page
			place {
				control = "WebPanel"
				start = "LibraryAnchor"
				dir = "down"
				height = "max"
				width = "max"
				x = "0"
				y = "0"
			}

			// New Library Page
			place {
				control = "NewLibraryPage"
				start = "LibraryAnchor"
				dir = "down"
				height = "max"
				width = "max"
				x = "0"
				y = "0"
			}

			// Console Page
			place {
				control = "ConsolePage"
				start = "LibraryAnchor"
				dir = "down"
				height = "max"
				width = "max"
				x = "0"
				y = "0"
			}

			
			// Music Page
			place {
				control = "MusicPage_Details"
				start = "LibraryAnchor"
				dir = "down"
				height = "max"
				width = "max"
				x = "0"
				y = "0"
			}

		//---------------------------------------------------------------------------------------------------------



		// Nav Bar ---------------------------------------------------------------------------------------------------------

			// Store - Library - Community - Profile - Console
			place {
				control = "label_store, label_library, label_community, label_me, label_console"
				height = "40"
				spacing = "10"
				x = "78"
				y = "10"
			}

			// Community "New" Tag
				place {
					control = "label_community_new"
					start = "label_community"
					height = "0"
					width = "0"
				}

		//---------------------------------------------------------------------------------------------------------



		// Forward & Back Buttons ---------------------------------------------------------------------------------------------------------

			// Back - Forward
			place {
				control = "back, forward"
				height = "40"
				width = "30"
				x = "10"
				y = "10"
			}

		//---------------------------------------------------------------------------------------------------------



		// Url Bar ---------------------------------------------------------------------------------------------------------

			// Refresh - Stop - Url Status Image - EvCert - Url Bar
			place {
				control = "refresh, stop, URLStatusImage, EVCert, URLBar"
				height = "24"
				width = "max"
				x = "10"
				y = "58"
			}

			// Page Load Throbber
			place {
				control = "PageLoadThrobber"
				align = "right"
				height = "30"
				width = "30"
				x = "10"
				y = "58"
			}

			// Url Anchor
			place {
				control = "URLAnchor"
				height = "24"
				width = "max"
				x = "0"
				y = "58"
			}

			// Library Anchor
			place {
				control = "LibraryAnchor"
				start = "URLAnchor"
				dir = "down"
				height = "24"
				width = "max"
				x = "0"
				y = "0"
			}
		//---------------------------------------------------------------------------------------------------------



		// Music Page ---------------------------------------------------------------------------------------------------------

			// Sub Nav Grouper
			place {
				control = "subnavgroup_library"
				height = "24"
				width = "max"
				x = "0"
				y = "58"
			}

			// Search Icon - Search Bar
			place {
				control = "label_search_icon, library_search"
				height = "24"
				spacing = "2"
				x = "10"
				y = "58"
			}

			// Filters
			place {
				control = "library_filters"
				align = "right"
				height = "24"
				width = "200"
				x = "10"
				y = "58"
			}

			// Filter Details - Filters List - Filters Grid
			place {
				control = "library_filters_details, library_filters_list, library_filters_grid"
				align = "right"
				height = "24"
				width = "20"
				spacing = "4"
				x = "10"
				y = "58"
			}
			
			// Music Player - Icon
			place {
				control = "library_music_player_icon"
				align = "right"
				height = "24"
				width = "20"
				spacing = "4"
				x = "10"
				y = "58"
			}

			// Music Menu - Music Add Button
			place {
				control = "library_music_menu, library_music_add_button"
				height = "24"
				spacing = "4"
				x = "290"
				y = "58"
			}

		//---------------------------------------------------------------------------------------------------------















































	}

}