"resource/layout/uinavigatorpanel.layout" {

	styles {

		//Navigation Buttons -------------------------------------------------------------------------------

			// Back Button -------------------------------------------------------------------------------

				// Back
				"BackButton" {
					bgcolor = "0 0 0 0"
					inset = "-6 0 0 0"
					image = "graphics/icon_button_back"
				}

				// Back - Disabled
				"BackButton:hover" {
					image = "graphics/icon_button_back_down"
				}

				// Back - Disabled
				"BackButton:active" {
					image = "graphics/icon_button_back_down"
				}

				// Back - Disabled
				"BackButton:disabled" {
					image = "graphics/icon_button_back_disabled"
				}

			// Forward -------------------------------------------------------------------------------

				// Forward
				"ForwardButton" {
					bgcolor = "0 0 0 0"
					inset = "-8 0 0 0"
					image = "graphics/icon_button_forward"
				}

				// Forward - Hover
				"ForwardButton:hover" {
					image = "graphics/icon_button_forward_over"
				}

				// Forward - Active
				"ForwardButton:active" {
					image = "graphics/icon_button_forward_down"
				}

				// Forward - Disabled
				"ForwardButton:disabled" {
					image = "graphics/icon_button_forward_disabled"
				}


			// Nav Buttons -------------------------------------------------------------------------------

				"CUINavButton" {
					font-size = "24"
					textcolor = "200 200 200 255"
					bgcolor = "0 0 0 0"
					padding-left = "4"
					padding-right = "4"
				}

				"CUINavButton:hover" {
					textcolor = "240 240 240 255"
				}
				
		// -------------------------------------------------------------------------------

		// URL Bar -------------------------------------------------------------------------------

			// Label
			"Label" {
				textcolor = "120 120 120 255"
				bgcolor = "0 0 0 0"
			}

			// Url Bar
			"TextEntryURL" {
				textcolor = "120 120 120 255"
				selectedtextcolor = "160 160 160 255"
				bgcolor = "0 0 0 0"
				selectedbgcolor = "30 30 30 255"
				padding-left = "8"
				padding-right = "8"
			}

			// Secure Url Bar
			"URLBarSecure" {
				textcolor = "126 166 75 255"
			}

			// Insecure Url Bar
			"URLBarBrokenSecure" {
				textcolor = "173 69 72 255"
			}

			// EVCert Label
			"LabelEVCert" {
				font-size = "14"
				textcolor = "30 30 30 255"
				bgcolor = "126 166 75 255"
				padding-left = "8"
				padding-right = "8"
			}

			// EVCert Lock Icon
			"URLStatusImage" {
				bgcolor = "0 0 0 0"
				image = "graphics/html_lock_disabled"
				padding-left = "8"
				padding-right = "8"
			}

			// Stop -------------------------------------------------------------------------------

				// Stop
				"StopButton" {
					bgcolor = "0 0 0 0"
					inset = "-5 0 0 0"
					image = "graphics/icon_button_stop"
					padding-left = "8"
					padding-right = "8"
				}

				// Stop - Hover
				"StopButton:hover" {
					image = "graphics/icon_button_stop_over"
				}

				// Stop - Active
				"StopButton:active" {
					image = "graphics/icon_button_stop_down"
				}

				// Stop - Disabled
				"StopButton:disabled" {
					image = "graphics/icon_button_stop_disabled"
				}

			// Refresh -------------------------------------------------------------------------------

				// Reload
				"ReloadButton" {
					bgcolor = "0 0 0 0"
					inset = "-5 0 0 0"
					image = "graphics/icon_button_reload"
					padding-left = "8"
					padding-right = "8"
				}

				// Reload - Hover
				"ReloadButton:hover" {
					image = "graphics/icon_button_reload_over"
				}

				// Reload - Active
				"ReloadButton:active" {
					image = "graphics/icon_button_reload_down"
				}

				// Reload - Disabled
				"ReloadButton:disabled" {
					image = "graphics/icon_button_reload_disabled"
				}


		// -------------------------------------------------------------------------------

	}

	layout {

		// Pages ---------------------------------------------------------------------------------------------------------

			// Web Page
			place {
				control = "WebPanel"
				height = "max"
				width = "max"
				margin-top = "90"
			}

			// New Library Page
			place {
				control = "NewLibraryPage"
				height = "max"
				width = "max"
				margin-top = "58"
			}

			// Console Page
			place {
				control = "ConsolePage"
				height = "max"
				width = "max"
				margin-top = "58"
			}

			
			// Music Page
			place {
				control = "MusicPage_Details"
				height = "max"
				width = "max"
				margin-top = "90"
			}

		//---------------------------------------------------------------------------------------------------------



		// Nav Bar ---------------------------------------------------------------------------------------------------------

			// Store - Library - Community - Profile - Console
			place {
				control = "label_store, label_library, label_community, label_me, label_console"
				height = "40"
				spacing = "8"
				x = "68"
				y = "10"
			}

			// Community "New" Tag
				place {
					control = "label_community_new"
					start = "label_community"
					height = "0"
					width = "0"
				}

		//---------------------------------------------------------------------------------------------------------



		// Forward & Back Buttons ---------------------------------------------------------------------------------------------------------

			// Back - Forward
			place {
				control = "back, forward"
				height = "40"
				x = "0"
				y = "10"
			}

		//---------------------------------------------------------------------------------------------------------



		// Url Bar ---------------------------------------------------------------------------------------------------------

			// Refresh - Stop - Url Status Image - EvCert - Url Bar
			place {
				control = "refresh, stop, URLStatusImage, EVCert, URLBar"
				spacing = "4"
				height = "24"
				width = "max"
				x = "0"
				y = "58"
			}

			// Page Load Throbber
			place {
				control = "PageLoadThrobber"
				align = "right"
				height = "30"
				width = "30"
				x = "10"
				y = "58"
			}

			// Url Anchor
			place {
				control = "URLAnchor"
				height = "24"
				width = "max"
				x = "0"
				y = "58"
			}

			// Library Anchor
			place {
				control = "LibraryAnchor"
				start = "URLAnchor"
				dir = "down"
				height = "24"
				width = "max"
				x = "0"
				y = "0"
			}
		//---------------------------------------------------------------------------------------------------------



		// Music Page ---------------------------------------------------------------------------------------------------------

			// Sub Nav Grouper
			place {
				control = "subnavgroup_library"
				height = "24"
				width = "max"
				x = "0"
				y = "58"
			}

			// Search Icon - Search Bar
			place {
				control = "label_search_icon, library_search"
				height = "24"
				spacing = "2"
				x = "10"
				y = "58"
			}

			// Filters
			place {
				control = "library_filters"
				align = "right"
				height = "24"
				width = "200"
				x = "10"
				y = "58"
			}

			// Filter Details - Filters List - Filters Grid
			place {
				control = "library_filters_details, library_filters_list, library_filters_grid"
				align = "right"
				height = "24"
				width = "20"
				spacing = "4"
				x = "10"
				y = "58"
			}
			
			// Music Player - Icon
			place {
				control = "library_music_player_icon"
				align = "right"
				height = "24"
				width = "20"
				spacing = "4"
				x = "10"
				y = "58"
			}

			// Music Menu - Music Add Button
			place {
				control = "library_music_menu, library_music_add_button"
				height = "24"
				spacing = "4"
				x = "290"
				y = "58"
			}

		//---------------------------------------------------------------------------------------------------------















































	}

}