"resource/layout/uinavigatorpanel.layout" {

	styles {

		// Navigation Panel ---------------------------------------------------------------------------------------------------------

			// Navigation Panel
			"CUINavigatorPanel" {
				bgcolor = "DialogBG"
			}

		// Nav Buttons ---------------------------------------------------------------------------------------------------------

			// Nav Button
			"CUINavButton" {
				textcolor = "Labelfocus"
				bgcolor = "none"
				font-family = "basefont"
				font-size = "24"
				font-weight = "400"
			}

		// Url ---------------------------------------------------------------------------------------------------------

			// Url - Ev Cert (Not OSX)
			"LabelEVCert" [!$OSX] {
				font-family = "basefont"
				font-size = "14"
				textcolor = "darkclientbg"
				font-weight = "400"
				bgcolor = "html.secureurl"
				inset-left = "4"
				inset-right = "4"
				inset-top = "3"
			}

			// Url - Ev Cert (OSX)
			"LabelEVCert" [$OSX] {
				font-family = "basefont"
				font-size = "14"
				textcolor = "darkclientbg"
				font-weight = "400"
				bgcolor = "html.secureurl"
				inset-left = "3"
				inset-right = "4"
				inset-top = "3"
			}

			// Url - Broken Secure
			"URLBarBrokenSecure" {
				textcolor = "html.insecureurl"
			}

			// Url - Secure
			"URLBarSecure" {
				textcolor = "html.secureurl"
			}

			// Url - Status Image
			"URLStatusImage" {
				bgcolor = "none"
				inset = "0 0 0 0"
				render_bg = {}
				image = "graphics/html_lock_disabled"
			}

		// Nav Label ---------------------------------------------------------------------------------------------------------

			// Nav Label
			"NavLabel" {
				font-family = "basefont"
				font-size = "14" [!$OSX]
				font-size = "15" [$OSX]
				font-weight = "400"
				textcolor = "Label"
				font-style = "uppercase"
			}

			// Nav Label 2 (Not OSX)
			"NavLabel2" [!$OSX] {
				font-family = "basefont"
				font-size = "14"
				font-weight = "400"
				textcolor = "labelfocus"
				font-style = "uppercase"
				}

			// Nav Label 2 (OSX)
			"NavLabel2" [$OSX] {
				font-family = "basefont"
				font-size = "13"
				font-weight = "400"
				textcolor = "labelfocus"
				font-style = "uppercase"
				}

			// Nav Label 2 - Hover
			"NavLabel2:hover" {
				textcolor = "texthover"
				font-style = "outerglow,uppercase"
				font-outerglow-color = "TextGlowHover"
				font-outerglow-offset = "1"
				font-outerglow-filtersize = "3"
			}

			// Nav Label 2 - Selected
			"NavLabel2:selected" {
				textcolor = "white"
				font-style = "outerglow,uppercase"
				font-outerglow-color = "TextGlowHover"
				font-outerglow-offset = "1"
				font-outerglow-filtersize = "3"
			}

			// Nav Label - View (Not OSX)
			"NavLabelView" [!$OSX] {
				font-family = "basefont"
				font-size = "14"
				font-weight = "400"
				textcolor = "LabelDisabled"
				font-style = "uppercase"
				padding-left = "0"
			}

			// Nav Label - View (OSX)
			"NavLabelView" [$OSX] {
				font-family = "basefont"
				font-size = "13"
				font-weight = "400"
				textcolor = "LabelDisabled"
				font-style = "uppercase"
				padding-left = "0"
			}

			// Nav Label - Zoom (Not OSX)
			"NavLabelZoom" [!$OSX] {
				font-family = "basefont"
				font-size = "14"
				font-weight = "400"
				textcolor = "LabelDisabled"
				font-style = "uppercase"
				padding-right = "5"
			}

			// Nav Label - Zoom (OSX)
			"NavLabelZoom" [$OSX] {
				font-family = "basefont"
				font-size = "13"
				font-weight = "400"
				textcolor = "LabelDisabled"
				font-style = "uppercase"
				padding-right = "5"
			}

		// Music ---------------------------------------------------------------------------------------------------------

			// Music - Add Button
			"MusicAddButton" {
				font-size = "14"
				font-size = "13" [$OSX]
				font-family = "basefont"
				textcolor = "labelfocus"
				font-style = "uppercase"
				render_bg {
					0 = "image( x0, y0 + 2, x1, y1, graphics/icon_collapse )"
				}
			}

			// Music - Add Button - Hover
			"MusicAddButton:hover" {
				textcolor = "white"
				render_bg {
					0 = "image( x0, y0 + 2, x1, y1, graphics/icon_collapse_over )"
				}
			}

			// Music - Player Icon
			"MusicPlayerIcon" {
				bgcolor = "none"
				inset = "-1 0 0 0"
				render_bg = {}
				image = "graphics/icon_music_player"
				padding-left = "0"
				padding-right = "-1"
				padding-top = "-1"
			}

			// Music - Player Icon - Hover
			"MusicPlayerIcon:hover" {
				bgcolor = "none"
				inset = "-1 0 0 0"
				render_bg = {}
				image = "graphics/icon_music_player_hover"
				padding-left = "0"
				padding-right = "-1"
				padding-top = "-1"
			}

			// Music - Player Icon - Selected
			"MusicPlayerIcon:selected" {
				bgcolor = "none"
				inset = "-1 0 0 0"
				render_bg = {}
				image = "graphics/icon_music_player_selected"
				padding-left = "0"
				padding-right = "-1"
				padding-top = "-1"
			}

		// Library ---------------------------------------------------------------------------------------------------------

			// Library - Search
			"LibrarySearch" {
				padding-left = "-4"
				font-family = "basefont"
				font-size = "14"
				font-weight = "400"
				textcolor = "text"
			}

			// Library - Search - Hover
			"LibrarySearch:hover" {
				textcolor = "texthover"
			}

			// Library - Search - Disabled
			"LibrarySearch:disabled" {
				font-style = "italic"
				textcolor = "None"
			}

			// Library - Search - Empty
			"LibrarySearch:empty" {
				font-style = "italic"
				textcolor = "LabelDisabled"
			}

			// Library - Search - Empty - Hover
			"LibrarySearch:empty:hover" {
				font-style = "italic"
				textcolor = "texthover"
			}

			// Library - Search - Icon
			"LibrarySearchIcon" {
				bgcolor = "none"
				inset = "-1 0 0 0"
				render_bg = {}
				image = "graphics/icon_button_search"
				padding-left = "0"
				padding-right = "-1"
				padding-top = "-1"
			}

			// Library - Search - Icon
			"LibrarySearchIcon:disabled" {
				bgcolor = "none"
				inset = "-2 0 0 0"
				render_bg = {}
				image = "none"
				padding-left = "0"
				padding-right = "-1"
			}

			// Library - Separator
			"LibrarySeparator" {
				padding-left = "0"
				font-family = "basefont"
				font-size = "16"
				font-weight = "400"
				textcolor = "LabelDisabled"
				inset = "0 -2 0 0"
			}

			// Library - Show (Not OSX)
			"LibraryShow" [!$OSX] {
				padding-left = "0"
				font-family = "basefont"
				font-size = "14"
				font-weight = "400"
				textcolor = "LabelDisabled"
				font-style = "uppercase"
			}

			// Library - Show (OSX)
			"LibraryShow" [$OSX] {
				padding-left = "0"
				font-family = "basefont"
				font-size = "13"
				font-weight = "400"
				textcolor = "LabelDisabled"
				font-style = "uppercase"
			}

		// Text Entry ---------------------------------------------------------------------------------------------------------

			// Text Entry
			"TextEntry" {
				font-family = "basefont"
				font-size = "14"
				textcolor = "Text"
				font-weight = "400"
				bgcolor = "none"
				selectedtextcolor = "white"
				selectedbgcolor = "TextSelectedBG"
				shadowtextcolor = "Text"
				inset-left = "4"
				inset-top = "0"
				render {}
			}

			// Text Entry - Hover
			"TextEntry:hover" {
				textcolor = "TextHover"
			}

			// Text Entry - Active
			"TextEntry:active" {
				textcolor = "TextHover"
			}

			// Text Entry - Selected
			"TextEntry:selected" {
				textcolor = "TextHover"
			}

			// Text Entry - Url (Not OSX)
			"TextEntryURL" [!$OSX] {
				font-family = "basefont"
				inset-top = "1"
				inset-left = "2"
				font-size = "14"
				textcolor = "label"
				font-weight = "400"
				bgcolor = "none"
				selectedtextcolor = "TextEntrySelected"
				selectedbgcolor = "TextSelectedBG"
				shadowtextcolor = "Text"
			}

			// Text Entry - Url (OSX)
			"TextEntryURL" [$OSX] {
				font-family = "basefont"
				inset-top = "1"
				inset-left = "2"
				font-size = "14"
				textcolor = "label"
				font-weight = "400"
				bgcolor = "none"
				selectedtextcolor = "TextEntrySelected"
				selectedbgcolor = "TextSelectedBG"
				shadowtextcolor = "Text"
			}

		// View Details ---------------------------------------------------------------------------------------------------------

			// View Details - Button
			"ViewDetailButton" {
				bgcolor = "none"
				inset = "1 0 0 0"
				render_bg = {}
				image = "graphics/icon_button_detail"
				padding-left = "0"
				padding-right = "0"
			}

			// View Details - Button - Hover
			"ViewDetailButton:hover" {
				bgcolor = "none"
				render_bg = {}
				image = "graphics/icon_button_detail_over"
			}

			// View Details - Button - Active
			"ViewDetailButton:active" {
				bgcolor = "none"
				render_bg = {}
				image = "graphics/icon_button_detail_down"
			}

			// View Details - Button - Selected
			"ViewDetailButton:selected" {
				bgcolor = "none"
				render_bg = {}
				image = "graphics/icon_button_detail_down"
			}

			// View Details - Button - Disabled
			"ViewDetailButton:disabled" {
				bgcolor = "ButtonFace"
				render_bg = {}
				image = "graphics/icon_button_detail_disabled"
			}

		// View Grid ---------------------------------------------------------------------------------------------------------


			// View Grid - Button
			"ViewGridButton" {
				bgcolor = "none"
				inset = "1 0 0 0"
				render_bg = {}
				image = "graphics/icon_button_grid"
				padding-left = "0"
				padding-right = "0"
			}

			// View Grid - Button - Hover
			"ViewGridButton:hover" {
				bgcolor = "none"
				render_bg = {}
				image = "graphics/icon_button_grid_over"
			}

			// View Grid - Button - Active
			"ViewGridButton:active" {
				bgcolor = "none"
				render_bg = {}
				image = "graphics/icon_button_grid_down"
			}

			// View Grid - Button - Selected
			"ViewGridButton:selected" {
				bgcolor = "none"
				render_bg = {}
				image = "graphics/icon_button_grid_down"
			}

			// View Grid - Button - Disabled
			"ViewGridButton:disabled" {
				bgcolor = "none"
				render_bg = {}
				image = "graphics/icon_button_grid_disabled"
			}

		// View List ---------------------------------------------------------------------------------------------------------

			// View List - Button
			"ViewListButton" {
				bgcolor = "none"
				inset = "1 0 0 0"
				render_bg = {}
				image = "graphics/icon_button_list"
				padding-left = "0"
				padding-right = "0"
			}

			// View List - Button - Hover
			"ViewListButton:hover" {
				bgcolor = "none"
				render_bg = {}
				image = "graphics/icon_button_list_over"
			}

			// View List - Button - Active
			"ViewListButton:active" {
				bgcolor = "none"
				render_bg = {}
				image = "graphics/icon_button_list_down"
			}

			// View List - Button - Selected
			"ViewListButton:selected" {
				bgcolor = "none"
				render_bg = {}
				image = "graphics/icon_button_list_down"
			}

			// View List - Button - Disabled
			"ViewListButton:disabled" {
				bgcolor = "none"
				render_bg = {}
				image = "graphics/icon_button_list_disabled"
			}

		// Other ---------------------------------------------------------------------------------------------------------


			// Grouper
			"grouper" {
				bgcolor = "none"
				render_bg {
					0 = "fill( x0 + 2, y0 + 1, x0 + 273, y1 - 1, clientgrouper )"
					1 = "fill( x0 + 279, y0 + 1, x1 - 10, y1 - 1, clientgrouper )"
				}
			}

			// Header Text Large
			"headertextlarge" {
				padding-left = "10"
				padding-top = "5"
				padding-right = "10"
				padding-bottom = "5"
				font-size = "30"
				textcolor = "Text"
				font-weight = "400"
				bgcolor = "DarkDialogBGTransparent"
				minimum-width = "200"
			}

			// Panel Transparent
			"Panel_transparent" {
				bgcolor = "none"
			}

			// Zoom Slider
			"zoomslider" {
				inset = "0 -1 0 0"
				textcolor = "navlabel"
			}

			// Zoom Slider - Hover
			"zoomslider:hover" {
				inset = "0 -1 0 0"
				textcolor = "navlabel"
			}


	}

	layout {

		// Pages ---------------------------------------------------------------------------------------------------------

			// Web Page
			place {
				control = "WebPanel"
				height = "100"
				width = "100"
				x = "0"
				y = "390"
			}

			// New Library Page
			place {
				control = "NewLibraryPage"
				height = "100"
				width = "100"
				x = "0"
				y = "390"
			}

			// Console Page
			place {
				control = "ConsolePage"
				height = "100"
				width = "100"
				x = "0"
				y = "390"
			}

			
			// Music Page
			place {
				control = "MusicPage_Details"
				height = "500"
				width = "500"
				x = "420"
				y = "170"
			}

		//---------------------------------------------------------------------------------------------------------



		// Nav Bar ---------------------------------------------------------------------------------------------------------

			// Store
			place {
				control = "label_store"
				height = "30"
				width = "60"
				x = "0"
				y = "350"
			}

			//Library
			place {
				control = "label_library"
				height = "30"
				width = "60"
				x = "70"
				y = "350"
			}

			//Community
			place {
				control = "label_community"
				height = "30"
				width = "60"
				x = "140"
				y = "350"
			}

			// Community "New" Tag
				place {
					control = "label_community_new"
					start = "label_community"
					width = "60"
					height = "30"
					x = "10"
				}

			//Profile (me)
			place {
				control = "label_me"
				height = "30"
				width = "60"
				x = "280"
				y = "350"
			}

			//Console
			place {
				control = "label_console"
				height = "30"
				width = "60"
				x = "350"
				y = "350"
			}

		//---------------------------------------------------------------------------------------------------------



		// Forward & Back Buttons ---------------------------------------------------------------------------------------------------------

			// Back
			place {
				control = "back"
				height = "30"
				width = "30"
				x = "120"
				y = "390"
			}

			// Forward
			place {
				control = "forward"
				height = "30"
				width = "30"
				x = "160"
				y = "390"
			}

		//---------------------------------------------------------------------------------------------------------



		// Url Bar ---------------------------------------------------------------------------------------------------------

			// Refresh
			place {
				control = "refresh"
				height = "30"
				width = "30"
				x = "120"
				y = "430"
			}

			// Stop
			place {
				control = "stop"
				height = "30"
				width = "30"
				x = "160"
				y = "430"
			}

			// Url Status Image
			place {
				control = "URLStatusImage"
				height = "30"
				width = "30"
				x = "200"
				y = "430"
			}

			// EvCert
			place {
				control = "EVCert"
				height = "30"
				width = "30"
				x = "240"
				y = "430"
			}

			// Url Bar
			place {
				control = "URLBar"
				height = "30"
				width = "30"
				x = "280"
				y = "430"
			}

			// Page Load Throbber
			place {
				control = "PageLoadThrobber"
				height = "30"
				width = "30"
				x = "320"
				y = "430"
			}

			// Url Anchor
			place {
				control = "URLAnchor"
				height = "30"
				width = "30"
				x = "360"
				y = "430"
			}
			
			// Library Anchor
			place {
				control = "LibraryAnchor"
				height = "30"
				width = "30"
				x = "400"
				y = "430"
			}

		//---------------------------------------------------------------------------------------------------------



		// Music Page ---------------------------------------------------------------------------------------------------------

			// Sub Nav Grouper
			place {
				control = "subnavgroup_library"
				height = "30"
				width = "60"
				x = "0"
				y = "310"
			}

			// Search Icon
			place {
				control = "label_search_icon"
				height = "30"
				width = "60"
				x = "70"
				y = "310"
			}

			// Search Bar
			place {
				control = "library_search"
				height = "30"
				width = "60"
				x = "140"
				y = "310"

			}

			// Filters
			place {
				control = "library_filters"
				height = "30"
				width = "60"
				x = "210"
				y = "310"
			}

			// Filter Details
			place {
				control = "library_filters_details"
				height = "30"
				width = "60"
				x = "280"
				y = "310"
			}

			// Filters List
			place {
				control = "library_filters_list"
				height = "30"
				width = "60"
				x = "280"
				y = "310"
			}

			// Filters Grid
			place {
				control = "library_filters_grid"
				height = "30"
				width = "60"
				x = "280"
				y = "310"
			}
			
			// Music Player - Icon
			place {
				control = "library_music_player_icon"
				height = "30"
				width = "60"
				x = "350"
				y = "310"
			}

			// Music Menu
			place {
				control = "library_music_menu"
				height = "30"
				width = "60"
				x = "0"
				y = "270"
			}

			// Music Add Button
			place {
				control = "library_music_add_button"
				height = "30"
				width = "60"
				x = "70"
				y = "270"
			}

		//---------------------------------------------------------------------------------------------------------















































	}

}