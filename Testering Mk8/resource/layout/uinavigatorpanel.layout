"resource/layout/uinavigatorpanel.layout" {

	colors {

		ClientBG = "30 30 30 255"
		DialogBG = "41 41 41 255"

	}

	styles {

		// Navigation Items ---------------------------------------------------------------------------------------------------------

			// Panel ---------------------------------------------------------------------------------------------------------

				// Panel
				"CUINavigatorPanel" {
					bgcolor = "none"
				}

			// Nav Buttons ---------------------------------------------------------------------------------------------------------

				// Nav Button (Not OSX)
				"CUINavButton" [!$OSX] {
					textcolor = "Labelfocus"
					bgcolor = "none"
					font-family = "basefont"
					font-size = "24"
					font-weight = "400"
					font-style = "uppercase"
					padding-left = "5"
					padding-right = "5"
				}

				// Nav Button (OSX)
				"CUINavButton" [$OSX] {
					textcolor = "Labelfocus"
					bgcolor = "none"
					font-family = "basefont"
					font-size = "24"
					font-weight = "400"
					font-style = "uppercase"
					padding-left = "5"
					padding-right = "5"
				}

				// Nav Button - Hover
				"CUINavButton:hover" {
					textcolor = "Text"
					font-style = "outerglow,uppercase"
					font-outerglow-color = "TextGlowHover"
					font-outerglow-offset = "1"
					font-outerglow-filtersize = "5"
				}

				// Nav Button - Selected
				"CUINavButton:selected" {
					textcolor = "white"
					bgcolor = "none"
					font-style = "outerglow,uppercase"
					font-outerglow-color = "TextGlowSelected"
					font-outerglow-offset = "1"
					font-outerglow-filtersize = "5"
				}

				// Nav Button - Disabled
				"CUINavButton:disabled" {
					textcolor = "TextDisabled"
					bgcolor = "none"
					font-family = "basefont"
					font-size = "26"
					font-weight = "400"
					font-style = "uppercase"
				}

				// Nav Button - Disabled - Hover
				"CUINavButton:disabled:hover" {
					textcolor = "TextDisabled"
					font-style = "uppercase"
				}

				// Nav Button - New
				"CUINavButtonNew" {
					textcolor = "133 139 148 255"
					font-weight = "800"
				}

				// Nav Button 2 (Not OSX)
				"CUINavButton2" [!$OSX] {
					textcolor = "Labelfocus"
					bgcolor = "none"
					font-family = "basefont"
					font-size = "15"
					font-weight = "400"
					font-style = "uppercase"
					padding-left = "5"
					padding-right = "5"
					padding-bottom = "4"
				}

				// Nav Button 2 (OSX)
				"CUINavButton2" [$OSX] {
					textcolor = "Labelfocus"
					bgcolor = "none"
					font-family = "basefont"
					font-size = "13"
					font-weight = "400"
					font-style = "uppercase"
					padding-left = "5"
					padding-right = "5"
					padding-bottom = "4"
				}

				// Nav Button 2 - Hover
				"CUINavButton2:hover" {
					textcolor = "text"
					font-style = "uppercase,outerglow"
					font-outerglow-color = "TextGlowHoverSm"
					font-outerglow-offset = "1"
					font-outerglow-filtersize = "1"
				}

				// Nav Button 2 - Selected
				"CUINavButton2:selected" {
					textcolor = "white"
					bgcolor = "none"
					font-style = "uppercase,outerglow"
					font-outerglow-color = "TextGlowSelectedSm"
					font-outerglow-offset = "1"
					font-outerglow-filtersize = "1"
				}

			// Games Page ---------------------------------------------------------------------------------------------------------

				// Games Page - Menu Button
				"CGamesPage_Mini CGameFilterMenuButton" {
					inset = "3 0 0 0"
					padding-left = "0"
				}

			// Root Dialog ---------------------------------------------------------------------------------------------------------

				// Root Dialog - Text Entry - Url
				"CSteamRootDialog TextEntryURL" {
					textcolor = "101 105 112 255"
				}

				// Root Dialog - Text Entry - Url - Hover
				"CSteamRootDialog TextEntryURL:hover" {
					textcolor = "texthover"
				}

				// Root Dialog - Game Filter Menu Button
				"CSteamRootDialog CGameFilterMenuButton" {
					inset = "3 0 0 0"
					padding-left = "0"
				}

			// Broadcast ---------------------------------------------------------------------------------------------------------

				// Broadcast - Panel Minimized
				"BroadcastPanelMinimized" {
					bgcolor = "none"
					textcolor = "label"
					inset = "0 0 0 0"
					render_bg {
						0 = "fill( x0, y0, x1, y1, almostblack )"
					}
				}

				// Broadcast - View
				"CBroadcastView" {
					inset = "0 0 0 0"
				}

			// Web Panel ---------------------------------------------------------------------------------------------------------

				// Web Panel
				"CWebPanel" {
					bgcolor = "none"
					textcolor = "label"
					inset = "1 1 1 1"
					render_bg {
						0 = "fill( x0, y0, x1, y1, almostblack )"
					}
				}

			// Url ---------------------------------------------------------------------------------------------------------

				// Url - Ev Cert (Not OSX)
				"LabelEVCert" [!$OSX] {
					font-family = "basefont"
					font-size = "14"
					textcolor = "darkclientbg"
					font-weight = "400"
					bgcolor = "html.secureurl"
					inset-left = "4"
					inset-right = "4"
					inset-top = "3"
				}

				// Url - Ev Cert (OSX)
				"LabelEVCert" [$OSX] {
					font-family = "basefont"
					font-size = "14"
					textcolor = "darkclientbg"
					font-weight = "400"
					bgcolor = "html.secureurl"
					inset-left = "3"
					inset-right = "4"
					inset-top = "3"
				}

				// Url - Broken Secure
				"URLBarBrokenSecure" {
					textcolor = "html.insecureurl"
				}

				// Url - Secure
				"URLBarSecure" {
					textcolor = "html.secureurl"
				}

				// Url - Status Image
				"URLStatusImage" {
					bgcolor = "none"
					inset = "0 0 0 0"
					render_bg = {}
					image = "graphics/html_lock_disabled"
				}

			// Nav Label ---------------------------------------------------------------------------------------------------------

				// Nav Label
				"NavLabel" {
					font-family = "basefont"
					font-size = "14" [!$OSX]
					font-size = "15" [$OSX]
					font-weight = "400"
					textcolor = "Label"
					font-style = "uppercase"
				}

				// Nav Label 2 (Not OSX)
				"NavLabel2" [!$OSX] {
					font-family = "basefont"
					font-size = "14"
					font-weight = "400"
					textcolor = "labelfocus"
					font-style = "uppercase"
					}

				// Nav Label 2 (OSX)
				"NavLabel2" [$OSX] {
					font-family = "basefont"
					font-size = "13"
					font-weight = "400"
					textcolor = "labelfocus"
					font-style = "uppercase"
					}

				// Nav Label 2 - Hover
				"NavLabel2:hover" {
					textcolor = "texthover"
					font-style = "outerglow,uppercase"
					font-outerglow-color = "TextGlowHover"
					font-outerglow-offset = "1"
					font-outerglow-filtersize = "3"
				}

				// Nav Label 2 - Selected
				"NavLabel2:selected" {
					textcolor = "white"
					font-style = "outerglow,uppercase"
					font-outerglow-color = "TextGlowHover"
					font-outerglow-offset = "1"
					font-outerglow-filtersize = "3"
				}

				// Nav Label - View (Not OSX)
				"NavLabelView" [!$OSX] {
					font-family = "basefont"
					font-size = "14"
					font-weight = "400"
					textcolor = "LabelDisabled"
					font-style = "uppercase"
					padding-left = "0"
				}

				// Nav Label - View (OSX)
				"NavLabelView" [$OSX] {
					font-family = "basefont"
					font-size = "13"
					font-weight = "400"
					textcolor = "LabelDisabled"
					font-style = "uppercase"
					padding-left = "0"
				}

				// Nav Label - Zoom (Not OSX)
				"NavLabelZoom" [!$OSX] {
					font-family = "basefont"
					font-size = "14"
					font-weight = "400"
					textcolor = "LabelDisabled"
					font-style = "uppercase"
					padding-right = "5"
				}

				// Nav Label - Zoom (OSX)
				"NavLabelZoom" [$OSX] {
					font-family = "basefont"
					font-size = "13"
					font-weight = "400"
					textcolor = "LabelDisabled"
					font-style = "uppercase"
					padding-right = "5"
				}

			// Music ---------------------------------------------------------------------------------------------------------

				// Music - Add Button
				"MusicAddButton" {
					font-size = "14"
					font-size = "13" [$OSX]
					font-family = "basefont"
					textcolor = "labelfocus"
					font-style = "uppercase"
					render_bg {
						0 = "image( x0, y0 + 2, x1, y1, graphics/icon_collapse )"
					}
				}

				// Music - Add Button - Hover
				"MusicAddButton:hover" {
					textcolor = "white"
					render_bg {
						0 = "image( x0, y0 + 2, x1, y1, graphics/icon_collapse_over )"
					}
				}

				// Music - Player Icon
				"MusicPlayerIcon" {
					bgcolor = "none"
					inset = "-1 0 0 0"
					render_bg = {}
					image = "graphics/icon_music_player"
					padding-left = "0"
					padding-right = "-1"
					padding-top = "-1"
				}

				// Music - Player Icon - Hover
				"MusicPlayerIcon:hover" {
					bgcolor = "none"
					inset = "-1 0 0 0"
					render_bg = {}
					image = "graphics/icon_music_player_hover"
					padding-left = "0"
					padding-right = "-1"
					padding-top = "-1"
				}

				// Music - Player Icon - Selected
				"MusicPlayerIcon:selected" {
					bgcolor = "none"
					inset = "-1 0 0 0"
					render_bg = {}
					image = "graphics/icon_music_player_selected"
					padding-left = "0"
					padding-right = "-1"
					padding-top = "-1"
				}

			// Library ---------------------------------------------------------------------------------------------------------

				// Library - Search
				"LibrarySearch" {
					padding-left = "-4"
					font-family = "basefont"
					font-size = "14"
					font-weight = "400"
					textcolor = "text"
				}

				// Library - Search - Hover
				"LibrarySearch:hover" {
					textcolor = "texthover"
				}

				// Library - Search - Disabled
				"LibrarySearch:disabled" {
					font-style = "italic"
					textcolor = "None"
				}

				// Library - Search - Empty
				"LibrarySearch:empty" {
					font-style = "italic"
					textcolor = "LabelDisabled"
				}

				// Library - Search - Empty - Hover
				"LibrarySearch:empty:hover" {
					font-style = "italic"
					textcolor = "texthover"
				}

				// Library - Search - Icon
				"LibrarySearchIcon" {
					bgcolor = "none"
					inset = "-1 0 0 0"
					render_bg = {}
					image = "graphics/icon_button_search"
					padding-left = "0"
					padding-right = "-1"
					padding-top = "-1"
				}

				// Library - Search - Icon
				"LibrarySearchIcon:disabled" {
					bgcolor = "none"
					inset = "-2 0 0 0"
					render_bg = {}
					image = "none"
					padding-left = "0"
					padding-right = "-1"
				}

				// Library - Separator
				"LibrarySeparator" {
					padding-left = "0"
					font-family = "basefont"
					font-size = "16"
					font-weight = "400"
					textcolor = "LabelDisabled"
					inset = "0 -2 0 0"
				}

				// Library - Show (Not OSX)
				"LibraryShow" [!$OSX] {
					padding-left = "0"
					font-family = "basefont"
					font-size = "14"
					font-weight = "400"
					textcolor = "LabelDisabled"
					font-style = "uppercase"
				}

				// Library - Show (OSX)
				"LibraryShow" [$OSX] {
					padding-left = "0"
					font-family = "basefont"
					font-size = "13"
					font-weight = "400"
					textcolor = "LabelDisabled"
					font-style = "uppercase"
				}

			// Text Entry ---------------------------------------------------------------------------------------------------------

				// Text Entry
				"TextEntry" {
					font-family = "basefont"
					font-size = "14"
					textcolor = "Text"
					font-weight = "400"
					bgcolor = "none"
					selectedtextcolor = "white"
					selectedbgcolor = "TextSelectedBG"
					shadowtextcolor = "Text"
					inset-left = "4"
					inset-top = "0"
					render {}
				}

				// Text Entry - Hover
				"TextEntry:hover" {
					textcolor = "TextHover"
				}

				// Text Entry - Active
				"TextEntry:active" {
					textcolor = "TextHover"
				}

				// Text Entry - Selected
				"TextEntry:selected" {
					textcolor = "TextHover"
				}

				// Text Entry - Url (Not OSX)
				"TextEntryURL" [!$OSX] {
					font-family = "basefont"
					inset-top = "1"
					inset-left = "2"
					font-size = "14"
					textcolor = "label"
					font-weight = "400"
					bgcolor = "none"
					selectedtextcolor = "TextEntrySelected"
					selectedbgcolor = "TextSelectedBG"
					shadowtextcolor = "Text"
				}

				// Text Entry - Url (OSX)
				"TextEntryURL" [$OSX] {
					font-family = "basefont"
					inset-top = "1"
					inset-left = "2"
					font-size = "14"
					textcolor = "label"
					font-weight = "400"
					bgcolor = "none"
					selectedtextcolor = "TextEntrySelected"
					selectedbgcolor = "TextSelectedBG"
					shadowtextcolor = "Text"
				}

			// View Details ---------------------------------------------------------------------------------------------------------

				// View Details - Button
				"ViewDetailButton" {
					bgcolor = "none"
					inset = "1 0 0 0"
					render_bg = {}
					image = "graphics/icon_button_detail"
					padding-left = "0"
					padding-right = "0"
				}

				// View Details - Button - Hover
				"ViewDetailButton:hover" {
					bgcolor = "none"
					render_bg = {}
					image = "graphics/icon_button_detail_over"
				}

				// View Details - Button - Active
				"ViewDetailButton:active" {
					bgcolor = "none"
					render_bg = {}
					image = "graphics/icon_button_detail_down"
				}

				// View Details - Button - Selected
				"ViewDetailButton:selected" {
					bgcolor = "none"
					render_bg = {}
					image = "graphics/icon_button_detail_down"
				}

				// View Details - Button - Disabled
				"ViewDetailButton:disabled" {
					bgcolor = "ButtonFace"
					render_bg = {}
					image = "graphics/icon_button_detail_disabled"
				}

			// View Grid ---------------------------------------------------------------------------------------------------------


				// View Grid - Button
				"ViewGridButton" {
					bgcolor = "none"
					inset = "1 0 0 0"
					render_bg = {}
					image = "graphics/icon_button_grid"
					padding-left = "0"
					padding-right = "0"
				}

				// View Grid - Button - Hover
				"ViewGridButton:hover" {
					bgcolor = "none"
					render_bg = {}
					image = "graphics/icon_button_grid_over"
				}

				// View Grid - Button - Active
				"ViewGridButton:active" {
					bgcolor = "none"
					render_bg = {}
					image = "graphics/icon_button_grid_down"
				}

				// View Grid - Button - Selected
				"ViewGridButton:selected" {
					bgcolor = "none"
					render_bg = {}
					image = "graphics/icon_button_grid_down"
				}

				// View Grid - Button - Disabled
				"ViewGridButton:disabled" {
					bgcolor = "none"
					render_bg = {}
					image = "graphics/icon_button_grid_disabled"
				}

			// View List ---------------------------------------------------------------------------------------------------------

				// View List - Button
				"ViewListButton" {
					bgcolor = "none"
					inset = "1 0 0 0"
					render_bg = {}
					image = "graphics/icon_button_list"
					padding-left = "0"
					padding-right = "0"
				}

				// View List - Button - Hover
				"ViewListButton:hover" {
					bgcolor = "none"
					render_bg = {}
					image = "graphics/icon_button_list_over"
				}

				// View List - Button - Active
				"ViewListButton:active" {
					bgcolor = "none"
					render_bg = {}
					image = "graphics/icon_button_list_down"
				}

				// View List - Button - Selected
				"ViewListButton:selected" {
					bgcolor = "none"
					render_bg = {}
					image = "graphics/icon_button_list_down"
				}

				// View List - Button - Disabled
				"ViewListButton:disabled" {
					bgcolor = "none"
					render_bg = {}
					image = "graphics/icon_button_list_disabled"
				}

			// Other ---------------------------------------------------------------------------------------------------------


				// Grouper
				"grouper" {
					bgcolor = "none"
					render_bg {
						0 = "fill( x0 + 2, y0 + 1, x0 + 273, y1 - 1, clientgrouper )"
						1 = "fill( x0 + 279, y0 + 1, x1 - 10, y1 - 1, clientgrouper )"
					}
				}

				// Header Text Large
				"headertextlarge" {
					padding-left = "10"
					padding-top = "5"
					padding-right = "10"
					padding-bottom = "5"
					font-size = "30"
					textcolor = "Text"
					font-weight = "400"
					bgcolor = "DarkDialogBGTransparent"
					minimum-width = "200"
				}

				// Panel Transparent
				"Panel_transparent" {
					bgcolor = "none"
				}

				// Zoom Slider
				"zoomslider" {
					inset = "0 -1 0 0"
					textcolor = "navlabel"
				}

				// Zoom Slider - Hover
				"zoomslider:hover" {
					inset = "0 -1 0 0"
					textcolor = "navlabel"
				}

		//---------------------------------------------------------------------------------------------------------

	}


	layout {

		// Navigator Panel ---------------------------------------------------------------------------------------------------------

			// Navigation - Buttons ---------------------------------------------------------------------------------------------------------

				// Navigation - Button - Back, Forward (Not OSX)
					place [!$OSX] {
						control = "back,forward"
						x = "3"
						y = "17"
						height = "30"
						width = "30"
						spacing = "2"
						margin-right = "5"
						margin-top = "0"
					}

				// Navigation - Button - Back, Forward (OSX)
					place [$OSX] {
						control = "back,forward"
						x = "3"
						y = "17"
						height = "30"
						width = "30"
						spacing = "2"
						margin-right = "5"
						margin-top = "0"
					}

				//---------------------------------------------------------------------------------------------------------

			// Navigation - Nav Bar ---------------------------------------------------------------------------------------------------------

				// Navigation - Nav Bar - Store, Library, Community, Profile (me), News, Console, Old Library
				place {
					control = "label_store, label_library, label_community, label_me, label_news, label_console, label_oldlibrary"
					x = "72"
					y = "9"
					spacing = "9"
					margin = "2"
					height = "40"
				}

				// Navigation - Nav Bar - Community Dropdown - Friends, Profile, Gamehubs, Forums, Workshop, Greenlight, Marketplace
				place {
					control = "label_community_friends, label_community_profile, label_community_gamehubs, label_community_forums, label_community_workshop, label_community_greenlight, label_community_marketplace"
					x = "84"
					y = "40"
					x = "10"
				}

				// Navigation - Nav Bar - Community "New" Tag
				place {
					control = "label_community_new"
					start = "label_community"
					y = "-40"
					x = "15"
					width = "0"
					height = "0"
				}

			// Navigation - Url Bar ---------------------------------------------------------------------------------------------------------

				// Url Bar - Page Load Throbber
				place {
					control = "PageLoadThrobber"
					align = "right"
					y = "54"
					height = "20"
					width = "20"
					margin-right = "9"
					margin-top = "1"
				}

				// Url Bar - Refresh, Stop, Url Status Image, EvCert, Url Bar
				place {
					control = "refresh,stop,URLStatusImage,EVCert,URLBar"
					align = "left"
					y = "54"
					height = "20"
					width = "max"
					spacing = "1"
					margin-left = "9"
				}

				// Url Bar - Url Anchor
				place {
					control = "URLAnchor"
					align = "left"
					y = "54"
					height = "28"
					width = "max"
				}
				
				// Url Bar - Library Anchor
				place {
					control = "LibraryAnchor"
					height = "28"
					width = "max"
					start = "URLAnchor"
					dir = "down"
				}

				// Url Bar - Email Reminder Bar
				place {
					control = "emailreminderbar"
					margin-top = "0"
					margin-left = "6"
					margin-right = "6"
					width = "max"
					height = "48"
					start = "LibraryAnchor"
					dir = "down"
				}

				// Url Bar - Phone Reminder Bar
				place {
					control = "phonereminderbar"
					margin-top = "0"
					margin-left = "0"
					margin-right = "6"
					width = "max"
					height = "80"
					start = "emailreminderbar"
					dir = "down"
				}

			// Music Page ---------------------------------------------------------------------------------------------------------

				// Library Zoom ---------------------------------------------------------------------------------------------------------

					// Zoom Label, Zoom, Zoom Separator, View (Not OSX)
					place [!$OSX] {
						control = "label_library_zoom,library_zoom,label_zoom_separator, label_library_view"
						align = "right"
						y = "55"
						height = "20"
						spacing = "3"
						margin-right = "85"
					}

					// Zoom Label, Zoom, Zoom Separator, View (OSX)
					place [$OSX] {
						control = "label_library_zoom,library_zoom,label_zoom_separator, label_library_view"
						align = "right"
						y = "55"
						height = "20"
						spacing = "3"
						margin-right = "84"
					}

					// Music Menu, Music Add Button
					place [!$OSX] {
						control = "library_music_menu,library_music_add_button"
						align = "left"
						x = "290"
						y = "55"
						height = "20"
						spacing = "3"
						margin-right = "180"
					}

					// Music Menu, Music Add Button
					place [$OSX] {
						control = "library_music_menu,library_music_add_button"
						align = "left"
						x = "290"
						y = "55"
						height = "20"
						spacing = "3"
						margin-right = "179"
					}

					// Zoom
					place {
						control = "library_zoom"
						width = "82"
						height = "20"
					}

				// Library Filter ---------------------------------------------------------------------------------------------------------

					// Region - Dropdown
					region {
						name = "dropdown"
						width = "200"
						x = "82"
						y = "55"
						height = "20"
					}

					// Filters
					place {
						control = "library_filters"
