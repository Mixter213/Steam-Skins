					render_bg {}
				}

				// Menu - Bar - Button - Focus - Selected
				"MenuBar MenuButton:frameFocus:selected" {
					textcolor = "texthover"
					bgcolor = none
					render_bg {}
				}

			// Menu - Dropdown - Items ------------------------------------------------------------------------------------------

				// Menu - Dropdown - Item
				MenuItem {
					font-family = "basefont"
					textcolor = "label"
					font-size = "14"
					font-weight = "400"
					inset = "0 0 0 0"
					padding-right = "6"
					bgcolor = "none"
