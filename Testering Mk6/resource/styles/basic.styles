				}
			}

			// Menu - Bar
			MenuBar {
				bgcolor = "dialogbg"
				padding-right = "1"
				padding-left = "6"
				padding-top = "2"
				padding-bottom = "-2"
			}

			// Menu - Button ------------------------------------------------------------------------------------------

